{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "i-tech-quest"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "./assets/libs/assets/animate.css/animate.css",
        "./assets/libs/assets/font-awesome/css/font-awesome.min.css",
        "./assets/libs/assets/simple-line-icons/css/simple-line-icons.css",
        "./assets/libs/jquery/bootstrap/dist/css/bootstrap.css",
        "../node_modules/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css",
        "./assets/css/app.min.css",
        "./assets/css/calender.css"
      ],
      "scripts": [
        "./assets/js/app.min.js",
        "./assets/js/jquery.easypiechart.fill.js",
        "./assets/js/jquery.sparkline.retina.js",
        "./assets/js/jquery.flot.js",
        "./assets/js/jquery.flot.pie.js",
        "./assets/js/jquery.flot.resize.js",
        "./assets/js/jquery.flot.tooltip.min.js",
        "./assets/js/jquery.flot.orderBars.js",
        "./assets/js/jquery.flot.spline.min.js",
        "./assets/js/moment.js",
        "./assets/js/underscore.js",
        "./assets/js/calender.js",
        "./assets/js/site.js",
        "../node_modules/bootstrap-notify/bootstrap-notify.js",
        "../node_modules/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}